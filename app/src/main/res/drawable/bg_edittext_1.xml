<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_window_focused="true">
        <shape android:shape="line">
            <stroke android:width="0.5dp" android:color="@color/btnColor" />
            <size android:height="0.5dp" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_window_focused="false">
        <shape android:shape="line">
            <stroke android:width="0.5dp" android:color="@color/black" />
            <size android:height="0.5dp" />
        </shape>
    </item>
</selector>