    <style name="ActivityStyle_Translucent" parent="AppTheme">
        <!--状态栏透明-->
        <item name="colorPrimaryDark">@android:color/transparent</item>
        <!--背景透明-->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="textAppearanceSmallPopupMenu">@style/TextAppearance_PopupMenu</item>
        <item name="textAppearanceLargePopupMenu">@style/TextAppearance_PopupMenu</item>

        <item name="textAllCaps">false</item>
    </style>
    <style name="AppTheme.NoWindowBackground">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="DialogStyle_MinWidth" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:windowMinWidthMinor">92%</item>
        <item name="android:windowMinWidthMajor">65%</item>
    </style>
    <style name="DialogStyle_MinWidth.NoTitle">
        <item name="windowNoTitle">true</item>
    </style>
    <style name="DialogStyle_NoTitle" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="windowNoTitle">true</item>
    </style>
    <style name="DialogStyle_PicturePreview" parent="@android:style/Theme.Dialog">
        <!-- Dialog的windowFrame框为无 -->
        <item name="android:windowFrame">@null</item>
        <!-- 不显示title -->
        <item name="android:windowNoTitle">true</item>
        <!-- 全屏 -->
        <item name="android:windowFullscreen">true</item>
        <!-- 设置黑色背景 -->
        <item name="android:windowBackground">@android:color/black</item>
    </style>
    <style name="TextAppearance_ActionBarTitle" ns1:keep="@style/TextAppearance_ActionBarTitle">
        <item name="android:textSize" ns1:ignore="SpUsage">
            @dimen/homepage_actionbar_title_text_size
        </item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextAppearance_PopupMenu" parent="TextAppearance.AppCompat.Menu">
        <item name="android:textSize">@dimen/primary_text_size</item>
    </style>
    <style name="TextAppearance_TabItem">
        <item name="android:textSize">@dimen/primary_text_size</item>
        <item name="android:textColor">@color/color_selector_tabtext</item>
    </style>
    <style name="TextAppearance_TrackTabItem">
        <item name="android:textSize">@dimen/track_selection_view_tab_text_size</item>
        <item name="android:textColor">@color/color_selector_track_tab_text</item>
    </style>
    <style name="TextAppearance_UpdateLogTitle">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/colorAccent</item>
    </style>
    <style name="WindowAnimations_BottomPopupWindow">
        <item name="android:windowEnterAnimation">@anim/anim_bottom_window_up</item>
        <item name="android:windowExitAnimation">@anim/anim_bottom_window_down</item>
