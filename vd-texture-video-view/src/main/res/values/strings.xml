    <!-- An option in a track selection list (e.g. a list view that allows the user to choose
         between multiple media tracks) to indicate that no track should be selected.
         Examples: When playing a video, if this option is selected for audio, playback will proceed
                   without sound. If this option is selected for video, playback will proceed
                   without visual output. -->
    <string name="track_selection_none">None</string>
    <!-- Name of a media track about which nothing is known. -->
    <string name="track_unknown">Unknown</string>
    <!-- Resolution of a video track. -->
    <string name="track_resolution" translatable="false"><xliff:g id="width" example="1024">%1$d</xliff:g> × <xliff:g id="height" example="768">%2$d</xliff:g></string>
    <!-- Describes the frame rate of a video track in frames per second. -->
    <string name="track_frameRate" translatable="false"><xliff:g id="frameRate" example="25">%1$s<!--.2f--></xliff:g> fps</string>
